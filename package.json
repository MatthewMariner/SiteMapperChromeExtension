{
  "name": "site-structure-navigator",
  "version": "1.0.1",
  "description": "Chrome extension to discover and navigate site structures",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "clean": "rimraf dist",
    "zip": "bestzip extension.zip dist/*",
    "generate-icons": "node utils/generate-icons.js",
    "prepare-screenshots": "node utils/prepare-screenshots.js",
    "prepare-promo": "node utils/prepare-promo-tiles.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "babel-loader": "^9.1.3",
    "bestzip": "^2.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "rimraf": "^5.0.1",
    "sharp": "^0.32.6",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "extpay": "^3.1.1"
  }
}
